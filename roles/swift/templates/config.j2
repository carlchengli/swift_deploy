[core]
        repositoryformatversion = 0
        filemode = true
        bare = false
        logallrefupdates = true
[remote "origin"]
        url = https://github.com/openstack/{{ project }}.git
        fetch = +refs/heads/*:refs/remotes/origin/*
[branch "master"]
        remote = origin
        merge = refs/heads/master
[user]
        name = {{ git.user }}
        email = {{ git.email }}
[remote "gerrit"]
        url = https://{{ gerrit.user }}:{{ gerrit.password }}@review.openstack.org/openstack/{{ project }}.git
        fetch = +refs/heads/*:refs/remotes/gerrit/*
